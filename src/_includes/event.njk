<div class="container1">
   
  <div class="row" >
    <div class="col-sm-4 ">
      
      
      <div class="poster">

        <img src="{{ poster }}" alt="">
        
      </div>
    
    
    </div>


    <div class="col" style="margin: 0%;padding: 0%;">
      
      <div class="con-1">
        <h1 > {{title}} </h1>

            <div class="con-txt">
          <p> {{long_description}} </p>
              </div> 

              <div class="con-details">
                <h3>Details</h3>

                <P><Span><b>DATE:</b></Span> {{date}} </P>
                 <P><Span><b>VENU:</b></Span> {{venu}} </P>
                <P><Span><b>Pre Requists:</b></Span>  {{prerequists}} </P>
              <P><Span><b>ENTRY FEE:</b></Span> {{fee}} </P>
                
              </div>

          
        </div> 
    
    
    </div>



    <div class="con-details2">
      <P><Span><b>Time:</b></Span> {{time}} </P>
      <P><Span><b>By Club:</b></Span> {{Club_name}} </P>
      <P><Span><b>Department:</b></Span> {{Department}} </P>
      <P><Span><b>Outcome:</b></Span> {{outcome}} </P>
      <Span><b>Contact:</b></Span><P> {{number_1}} <br> {{number_2}} </P>


    </div>


  </div>

  <div class="row">
    <div class="col">
      
